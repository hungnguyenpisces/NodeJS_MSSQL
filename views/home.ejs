                    <form class="row">
                        <select class="form-control col-md-3 mr-1" name="database" id="db-name"
                            onchange="handleChange()" required>
                            <option value="" disabled selected hidden>Select from Database</option>
                            <option value="Ass5">Database Ass5</option>
                            <option value="LabS6">Database LabS6</option>
                            <option value="Ass6">Database Ass6</option>
                            <option value="LabS7">Database LabS7</option>
                            <option value="LabS8">Database LabS8</option>
                            <option value="W3DB">Database W3</option>
                        </select>
                        <select class="form-control col-md-2 mr-1" name="table" id="table-name" required>
                            <option value="" disabled selected hidden>Select from table</option>
                        </select>
                        <button class="btn btn-primary col-md-2 mr-1" onclick="selectdb()">Load Database</button>
                        <button class="btn btn-success col-md-1 mr-1" type="button" onclick="addbtn()">Add</button>
                        <button class="btn btn-warning col-md-1 mr-1" type="button"
                            onclick="updatebtn()">Update</button>
                        <button class="btn btn-danger col-md-1 mr-1" type="button" onclick="deletebtn()">Delete</button>
                        <button class="btn btn-info col-md-1" type="button" onclick="searchbtn()">Search</button>
                    </form>
                    <form class="row mt-3" id="add-db" style="display: none;">
                        <div class="input-group" id="add-db-row">
                        </div>
                    </form>
                    <form class="row mt-3" id="update-db" style="display: none;">
                        <div class="input-group">
                            <select class="col-md-3 form-control" name="column" id="update-col-name1" required>
                                <option value="" disabled selected>Update on column</option>
                            </select>
                            <input type="text" class="form-control" id="update1" name="keyword" placeholder="Value..."
                                required>
                            <select class="col-md-3 form-control" name="column" id="update-col-name2" required>
                                <option value="" disabled selected>Column to reference</option>
                            </select>
                            <input type="text" class="form-control" id="update2" name="keyword" placeholder="Value..."
                                required>
                            <div class="input-group-append">
                                <button class="btn btn-warning">Update</button>
                            </div>
                        </div>
                    </form>
                    <form class="row mt-3" id="delete-db" style="display: none;">
                        <div class="input-group">
                            <select class="col-md-3 form-control" name="column" id="delete-col-name" required>
                                <option value="" disabled selected>Delete on column</option>
                            </select>
                            <input type="text" class="form-control" id="delete" name="keyword" placeholder="Value..."
                                required>
                            <div class="input-group-append">
                                <button class="btn btn-danger">Delete</button>
                            </div>
                        </div>
                    </form>
                    <form class="row mt-3" id="search-db" style="display: none;">
                        <div class="input-group">
                            <select class="col-md-3 form-control" name="column" id="search-col-name" required>
                                <option value="" disabled selected>Search on column</option>
                            </select>
                            <input type="text" class="form-control" id="keyword" name="keyword" placeholder="Search..."
                                required>
                            <div class="input-group-append">
                                <button class="btn btn-info" onclick="searchEnter()">Search</button>
                            </div>
                        </div>
                    </form>
