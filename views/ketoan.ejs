<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Kế toán</title>
    <!-- Latest compiled and minified CSS -->
    <%- include("head")%>
    <link rel="stylesheet" href="css/demobootstrap.css" type="text/css"/>
    <link rel="stylesheet" href="css/assignment3.css" type="text/css"/>
</head>
<body>
<%- include("header")%>
<%- include("nav")%>

<section class="main col-md-12">
    <div class="container">
        <div class="grid">
            <div class="row">
                <% for(var i=0;i<products.length;i++){ %>
                    <div class="item">
                        <a href="/chi-tiet?id=<%= products[i].id %>">
                            <img src="<%= products[i].image %>"/>
                        </a>
                        <a href="/chi-tiet2/<%= products[i].id %>">
                            <h2><%= products[i].name %></h2>
                        </a>
                    </div>
                <% } %>


            </div>

        </div>
    </div>
</section>
<%- include("footer") %>
</body>
</html>